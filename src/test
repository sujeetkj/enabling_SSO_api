public static GridSummary toDto(ModelPtfolInptParm e) {
        GridSummary dto = new GridSummary();
        dto.setBatchId(e.getKey().intValue());                 // or Long if you prefer
        dto.setRunId(e.getRunId());
        dto.setReportDate(formatDate(e.getReportDate()));
        dto.setSubmissionDate(formatDateTime(e.getJobSubmitDt()));
        dto.setReportRunDate(formatDateTime(e.getJobCmpltDt())); // or MOD_TS
        dto.setSubmittedBy(e.getCreatedBy());
        dto.setStatus(e.getJobStatusCd());                     // map codeâ†’label if needed
        dto.setErrorMsg(null);
        dto.setComments(null);
        return dto;
    }
